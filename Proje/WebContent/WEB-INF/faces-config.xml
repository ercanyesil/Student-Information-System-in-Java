<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <managed-bean>
  <managed-bean-name>ogrenciKayitView</managed-bean-name>
  <managed-bean-class>OgrenciKayitView</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>dersKayitView</managed-bean-name>
  <managed-bean-class>DersKayitView</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>giris</managed-bean-name>
  <managed-bean-class>Giris</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/Giris.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>dogru</from-outcome>
   <to-view-id>/DersKayit.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>yanlis</from-outcome>
   <to-view-id>/OgrenciKayit.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
</faces-config>
